parameters:
  level: max
  checkMissingCallableSignature: true
  tipsOfTheDay: false
  errorFormat: compact
  parallel:
    maximumNumberOfProcesses: 7
  paths:
    - src
  ignoreErrors:
    - message: '/^Call to an undefined method Negotiation\\AcceptHeader\|Negotiation\\BaseAccept::getValue\(\)\.$/'
      path: src/Provider.php
