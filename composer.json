{
  "name": "juliangut/negotiate",
  "description": "Negotiation middleware",
  "keywords": [
    "negotiator",
    "middleware"
  ],
  "homepage": "https://github.com/juliangut/negotiate",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Julián Gutiérrez",
      "email": "juliangut@gmail.com",
      "homepage": "http://juliangut.com",
      "role": "Developer"
    }
  ],
  "support": {
    "source": "https://github.com/juliangut/negotiate",
    "issues": "https://github.com/juliangut/negotiate/issues"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.3|^8.0",
    "psr/http-server-middleware": "^1.0",
    "willdurand/negotiation": "^3.0"
  },
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7",
    "friendsofphp/php-cs-fixer": "^2.17",
    "infection/infection": "^0.18|^0.21",
    "laminas/laminas-diactoros": "^2.2",
    "overtrue/phplint": "^2.3",
    "phpmd/phpmd": "^2.9",
    "phpcompatibility/php-compatibility": "^9.3",
    "phpstan/extension-installer": "^1.1.0",
    "phpstan/phpstan": "^0.12",
    "phpstan/phpstan-deprecation-rules": "^0.12",
    "phpstan/phpstan-strict-rules": "^0.12",
    "phpunit/phpunit": "^9.5",
    "povils/phpmnd": "^2.4",
    "roave/security-advisories": "dev-master",
    "sebastian/phpcpd": "^6.0.3",
    "squizlabs/php_codesniffer": "^3.5",
    "thecodingmachine/phpstan-strict-rules": "^0.12"
  },
  "autoload": {
    "psr-4": {
      "Jgut\\Negotiate\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jgut\\Negotiate\\Tests\\": "tests/Negotiate/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  }
}
